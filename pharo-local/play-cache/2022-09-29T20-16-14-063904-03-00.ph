|cadena n cadMIN cadMAY|cadena:= (UIManager default request: 'Ingrese una cadena').n:= (cadena size).1 to: n do: [ :i|	((cadena at: i) isLowercase ) ifTrue: [ 		cadMAY := cadMAY , ((cadena at: i) asUppercase ) asString  ]											ifFalse: [ 		cadMAY := cadMAY , (cadena at: i) asString ]	 ].1 to: n do: [ :i|	(((cadena at: i) isUppercase )) ifTrue: [ 		cadMIN := cadMIN , ((cadena at: i) asLowercase ) asString ]											ifFalse: [ 		cadMIN := cadMIN , ((cadena at: i) asString ) ]	 ].UIManager inform: 'La cadena ingresada:   ', cadena.UIManager inform: 'La cadena en mayusculas:    ', cadMAY.UIManager inform: 'La cadena en minusculas:     ', cadMIN.