|n arreglo i m mul2 mul3 mul5 mul7|m:=0. mul2:=0. mul3 :=0. mul5 :=0. mul7 :=0.n:= (UIManager default request: 'Ingrese la cantidad de numeros que desea ingresar:  ') asInteger.arreglo:= Array new: n.1 to:n do: [:j| 	i:= (UIManager default request: 'Ingrese un valor:  ') asInteger.	arreglo at: j put: i.	 ].1 to: n do:[ :x|	m:= arreglo at: x.	((m \\ 2) == 0) ifTrue: [ mul2:= mul2 + 1 ].	((m \\ 3) == 0) ifTrue: [ mul3 := mul3 + 1 ].	((m \\ 5) == 0) ifTrue: [ mul5 := mul5 + 1 ].	((m \\ 7) == 0) ifTrue: [ mul7 := mul7 + 1 ]		].	UIManager inform: 'Multiplos de 2:   ', mul2 asString.UIManager inform: 'Multiplos de 3:   ', mul3 asString.UIManager inform: 'Multiplos de 5:   ', mul5 asString.UIManager inform: 'Multiplos de 7:   ', mul7 asString.