|arreglo i j aux unJugador x x1 x2 mov movimiento c cj|arreglo:=Array new: 4.arreglo at:1 put:(Array new:4).arreglo at:2 put:(Array new:4).arreglo at:3 put:(Array new:4).arreglo at:4 put:(Array new:4).(arreglo at:1) at:1 put: 3.(arreglo at:1) at:2 put: 2.(arreglo at:1) at:3 put: 2.(arreglo at: 1) at: 4 put:0.(arreglo at:2) at:1 put:0.(arreglo at:2) at:2 put:1.(arreglo at:2) at:3 put: 2.(arreglo at:2) at:4 put:0.(arreglo at:3) at:1 put: 0.(arreglo at:3) at:2 put: 1.(arreglo at:3) at:3 put: 2.(arreglo at:3) at:4 put: 0.(arreglo at:4) at:4 put: 4.(arreglo at:4) at:3 put: 1.(arreglo at:4) at:2 put: 1.(arreglo at:4) at:1 put: 0.unJugador := 2.movimiento:= 'H'.i:=1. j:=1. c:=0. cj:=0. mov:=0. aux:=0.(movimiento == 'H') ifTrue:[	[ i < 5 ] whileTrue: [ 		[ j < 4 ] whileTrue: [ 				aux:= ((arreglo at:i)at:j).				(aux == unJugador ) ifTrue: [ 							cj:= cj +1.					 ].				(aux == 0) ifTrue: [ 							c:= c +1.					 ].			j:= j+1.			 ] "fin columnas"		 ]. "fin filas"							]. Transcript show: cj.Transcript show: c.	