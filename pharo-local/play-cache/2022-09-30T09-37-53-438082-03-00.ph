|cad1 cad2 sal i var| cad1:=(UIManager default request:'Ingrese primer cadena')substrings.cad2:=(UIManager default request:'Ingrese seg cadena') substrings.var:=''. sal:=''.(cad1 size>=cad2 size ) ifTrue:[i:=cad2 size]				ifFalse:[i:=cad1 size].		1 to:i do:[:j| sal:=sal, (cad1 at:j)asString.							sal:=sal, ' 'asString.							sal:=sal, (cad2 at:j)asString.							sal:=sal, ' 'asString ].				^sal 