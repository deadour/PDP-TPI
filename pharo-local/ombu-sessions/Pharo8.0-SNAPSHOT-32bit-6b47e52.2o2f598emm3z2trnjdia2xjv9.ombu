ado\' ].\r\tceldaModelo := Celda new.\r\tancho := celdaModelo width.\r\talto := celdaModelo height.\r\tself "Fija l√≠mites del Morph para el tablero"\r\t\tbounds: (0 @ 0 extent: (ancho * n) @ (alto * n) + (2 * self borderWidth)).\r\tTable := Tablero \t"Se crea el tablero"\r\t\tnew: n\r\t\ttabulate: [ :i :j | self nuevaCeldaAt: i at: j ].\r\tTable minar: Game cantMinas.\r\tGame timer: Cronometro new iniciar.\r\tVentana := self openInWindow setLabel: \'Buscaminas\'',			#stamp : 'AlanZanazzo 11/23/2020 10:14',			#package : #Buscaminas-Game		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2022-11-18T22:07:24.749469-03:00' ],		#prior : OmReference [ '40' ],		#self : OmReference [ '41' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #'Buscaminas class',				#isMetaSide : true			},			#name : #gameOver,			#protocol : #finalization,			#sourceCode : 'gameOver\t\r\t| duracion |\r\tduracion := Game duracion.\r\tUIManager default \talert: \'BOOM !!!\'.\r\tUIManager default \r\t\tmessage: \'GAME OVER\'\r\t\ttitle: \'You Lose\'.\r\tUIManager default inform: \'Game Over\'.\r\tUIManager default \r\t\tmessage: duracion asString\r\t\ttitle: \'Duraci√≥n de la partida\'.\r\tGame jugador ganador: false.\r\tGame registrarPartida.\r\tVentana close',			#stamp : 'AlanZanazzo 11/22/2020 22:35',			#package : #Buscaminas-Game		}	}}OmEntry {	#tags : {		#author : 'Unknown',		#time : DateAndTime [ '2022-11-18T22:07:24.755469-03:00' ],		#prior : OmReference [ '41' ],		#self : OmReference [ '42' ]	},	#content : EpMethodAddition {		#method : RGMethodDefinition {			#annotations : IdentityDictionary {				#className : #'Buscaminas class',				#isMetaSide : true			},			#name : #gameWinner,			#protocol : #finalization,			#sourceCode : 'gameWinner\r\t| duracion |\r\tduracion := Game duracion.\r\tUIManager default \r\t\tmessage: \'FELICITACIONES GAN√ì LA PARTIDA !!!\'\r\t\